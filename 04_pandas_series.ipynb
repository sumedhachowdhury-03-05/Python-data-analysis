# Pandas Series - 2nd Year Level

import pandas as pd

print("PANDAS SERIES\n")

# Creating a Series from a list
marks = [85, 90, 78, 92]
series_marks = pd.Series(marks)

print("Series from list:")
print(series_marks)
print("\n")

# Creating a Series with custom index
subjects = ["Maths", "Science", "English", "Computer"]
series_subjects = pd.Series(marks, index=subjects)

print("Series with custom index:")
print(series_subjects)
print("\n")

# Accessing data
print("Marks in Science:", series_subjects["Science"])
print("\n")

# Basic Operations
print("Add 5 to each mark:")
print(series_subjects + 5)
print("\n")

# Statistical functions
print("Average Marks:", series_subjects.mean())
print("Maximum Marks:", series_subjects.max())
print("Minimum Marks:", series_subjects.min())

