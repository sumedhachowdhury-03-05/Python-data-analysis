# Data Analysis using Pandas - 2nd Year Level

import pandas as pd

print("DATA ANALYSIS\n")

# Creating DataFrame
data = {
    "Department": ["IT", "IT", "HR", "HR", "Finance", "Finance"],
    "Employee": ["Aarav", "Diya", "Rahul", "Meera", "Karan", "Neha"],
    "Salary": [50000, 55000, 45000, 48000, 60000, 62000]
}

df = pd.DataFrame(data)

print("Employee Data:")
print(df)
print("\n")

# GroupBy Operation
dept_salary = df.groupby("Department")["Salary"].mean()

print("Average Salary by Department:")
print(dept_salary)
print("\n")

# Sorting
sorted_df = df.sort_values(by="Salary", ascending=False)

print("Employees sorted by Salary:")
print(sorted_df)
print("\n")

# Simple Insight
highest_paid = sorted_df.iloc[0]
print("Highest Paid Employee:")
print(highest_paid)

