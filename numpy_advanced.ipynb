# NumPy Advanced - 2nd Year Level

import numpy as np

print("NUMPY ADVANCED\n")

# Creating Arrays
arr = np.array([1, 2, 3, 4, 5, 6])

print("Original Array:", arr)
print("\n")

# Reshaping Array
reshaped_arr = arr.reshape(2, 3)
print("Reshaped Array (2x3):\n", reshaped_arr)
print("\n")

# Broadcasting
broadcast_result = reshaped_arr + 10
print("Broadcasting (Add 10):\n", broadcast_result)
print("\n")

# Universal Functions (ufuncs)
print("Square Root:\n", np.sqrt(reshaped_arr))
print("Exponential:\n", np.exp(reshaped_arr))
print("\n")

# Aggregation Functions
print("Sum:", np.sum(reshaped_arr))
print("Mean:", np.mean(reshaped_arr))
print("Standard Deviation:", np.std(reshaped_arr))
print("\n")

# Random Numbers
random_arr = np.random.randint(1, 100, size=(3, 3))
print("Random Array:\n", random_arr)

