# Pandas DataFrame - 2nd Year Level

import pandas as pd

print("PANDAS DATAFRAME\n")

# Creating DataFrame from dictionary
data = {
    "Name": ["Aarav", "Diya", "Rahul", "Meera"],
    "Age": [19, 20, 18, 21],
    "Marks": [85, 90, 78, 92]
}

df = pd.DataFrame(data)

print("Student DataFrame:")
print(df)
print("\n")

# Accessing Columns
print("Names:")
print(df["Name"])
print("\n")

# Accessing Rows
print("First two rows:")
print(df.loc[0:1])
print("\n")

# Filtering Data
print("Students with Marks > 85:")
print(df[df["Marks"] > 85])
print("\n")

# Adding New Column
df["Result"] = ["Pass", "Pass", "Pass", "Pass"]
print("DataFrame after adding Result column:")
print(df)

