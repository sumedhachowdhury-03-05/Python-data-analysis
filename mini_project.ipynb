# Mini Project: Student Performance Analysis
# 2nd Year Level

import pandas as pd
import matplotlib.pyplot as plt

print("STUDENT PERFORMANCE ANALYSIS - MINI PROJECT\n")

# Creating dataset
data = {
    "Name": ["Aarav", "Diya", "Rahul", "Meera", "Karan"],
    "Maths": [85, 90, 78, 92, 88],
    "Science": [80, 95, 75, 90, 85],
    "English": [78, 88, 72, 85, 80]
}

df = pd.DataFrame(data)

print("Student Marks Data:")
print(df)
print("\n")

# Calculating total and average
df["Total"] = df[["Maths", "Science", "English"]].sum(axis=1)
df["Average"] = df["Total"] / 3

print("After Calculations:")
print(df)
print("\n")

# Finding Top Performer
topper = df.loc[df["Average"].idxmax()]
print("Top Performer:")
print(topper)
print("\n")

# Visualization - Average Marks
plt.figure()
plt.bar(df["Name"], df["Average"])
plt.title("Average Marks of Students")
plt.xlabel("Student Name")
plt.ylabel("Average Marks")
plt.show()

# Conclusion
print("Conclusion:")
print("Meera is the top performer based on average marks.")

