# Data Visualization Basics - 2nd Year Level

import pandas as pd
import matplotlib.pyplot as plt

print("DATA VISUALIZATION\n")

# Creating DataFrame
data = {
    "Subject": ["Maths", "Science", "English", "Computer"],
    "Marks": [85, 90, 78, 92]
}

df = pd.DataFrame(data)

print("Student Marks Data:")
print(df)
print("\n")

# Line Plot
plt.figure()
plt.plot(df["Subject"], df["Marks"])
plt.title("Marks Line Plot")
plt.xlabel("Subject")
plt.ylabel("Marks")
plt.show()

# Bar Chart
plt.figure()
plt.bar(df["Subject"], df["Marks"])
plt.title("Marks Bar Chart")
plt.xlabel("Subject")
plt.ylabel("Marks")
plt.show()

# Histogram
plt.figure()
plt.hist(df["Marks"])
plt.title("Marks Distribution")
plt.xlabel("Marks")
plt.ylabel("Frequency")
plt.show()

