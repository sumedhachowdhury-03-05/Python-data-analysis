# NumPy Basics - 2nd Year Level

import numpy as np

print("NUMPY BASICS\n")

# Creating NumPy Arrays
arr1 = np.array([10, 20, 30, 40, 50])
arr2 = np.array([[1, 2, 3], [4, 5, 6]])

print("1D Array:", arr1)
print("2D Array:\n", arr2)
print("\n")

# Array Properties
print("Shape of arr2:", arr2.shape)
print("Data type of arr1:", arr1.dtype)
print("\n")

# Indexing and Slicing
print("First element of arr1:", arr1[0])
print("Last two elements of arr1:", arr1[-2:])
print("\n")

# Mathematical Operations
print("Array + 5:", arr1 + 5)
print("Array * 2:", arr1 * 2)
print("\n")

# Statistical Operations
print("Sum:", np.sum(arr1))
print("Mean:", np.mean(arr1))
print("Maximum:", np.max(arr1))
print("Minimum:", np.min(arr1))

